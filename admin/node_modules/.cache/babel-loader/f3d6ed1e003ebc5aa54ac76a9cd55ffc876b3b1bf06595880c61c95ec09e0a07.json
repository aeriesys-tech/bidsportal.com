{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.every.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.for-each.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport \"core-js/modules/esnext.iterator.some.js\";\nexport default {\n  name: \"TreeItem\",\n  props: {\n    item: Object,\n    tdr_naics: Array,\n    search: String,\n    clear_all_naics: Boolean\n  },\n  watch: {\n    clear_all_naics: function () {\n      this.item.selected = false;\n    },\n    \"$store.getters.selected_naics\": {\n      handler() {\n        this.isSeletedItem();\n      },\n      immediate: true,\n      deep: true\n    },\n    item: {\n      handler() {\n        if (this.item.naics_code == \"Select All\") {\n          if (\"children\" in this.item && this.item.children.length) {\n            const all_selected = this.item.children.every(child => child.selected);\n            this.item.selected = all_selected;\n          } else {\n            this.item.selected = false;\n          }\n          this.$store.dispatch(\"setAllNaics\", this.item.selected);\n        }\n      },\n      immediate: true,\n      deep: true\n    },\n    search: {\n      handler(newSearch) {\n        if (newSearch) {\n          this.checkForHighlight(this.item, newSearch);\n        }\n      },\n      immediate: true\n    }\n  },\n  data() {\n    return {\n      isOpen: false,\n      item_tdr_naics: []\n    };\n  },\n  computed: {\n    isFolder: function () {\n      return this.item.children && this.item.children?.length;\n    }\n  },\n  mounted() {\n    if (!this.item.name) this.isOpen = true;else this.isOpen = this.item.isOpen;\n  },\n  methods: {\n    checkForHighlight(item, search) {\n      if (!search) {\n        // Reset to the initial state when search is cleared\n        this.isOpen = this.item.isOpen;\n        if (this.isFolder) {\n          item.children.forEach(child => {\n            child.isOpen = child.isOpen;\n            this.checkForHighlight(child, search); // Recursive call to reset all children\n          });\n        }\n        return false;\n      }\n      let isMatch = item.name && item.name.toLowerCase().includes(search.toLowerCase());\n      if (this.isFolder) {\n        let childMatches = item.children.some(child => this.checkForHighlight(child, search));\n        if (isMatch || childMatches) {\n          this.isOpen = true;\n          this.expandParents(this);\n        } else {\n          this.isOpen = false;\n        }\n        return isMatch || childMatches;\n      } else {\n        return isMatch;\n      }\n    },\n    expandParents(item) {\n      if (item.$parent && item.$parent.isFolder) {\n        item.$parent.isOpen = true;\n        this.expandParents(item.$parent);\n      }\n    },\n    isSeletedItem() {\n      const selected_naics = this.$store.getters.selected_naics;\n      if (selected_naics?.length) {\n        this.item.selected = selected_naics.includes(this.item.naics_id);\n      } else {\n        this.item.selected = false;\n      }\n    },\n    toggle: function () {\n      let vm = this;\n      if (vm.isFolder) {\n        vm.isOpen = !vm.isOpen;\n      }\n      if (\"children\" in vm.item && vm.item.children.length) {\n        if (vm.item.selected) {\n          vm.item.children.map(function (element) {\n            element.selected = true;\n            vm.pushSpliceItem(element);\n          });\n        }\n        vm.pushSpliceItem(this.item);\n      }\n    },\n    highlight(name) {\n      if (!this.search) {\n        return '<span class=\"hovertext\" style=\"color: rgb(89, 93, 110);font-size:14px\">' + name + \"</span>\";\n      }\n      name = name.replace(new RegExp(this.search, \"gi\"), match => {\n        return '<span class=\"hovertext\" style=\"background:yellow;font-size:14px\">' + match + \"</span>\";\n      });\n      return name;\n    },\n    toggleSelectedItem(item, event) {\n      let vm = this;\n      item.selected = !item.selected;\n      vm.pushSpliceItem(item);\n      if (event.target.checked) {\n        if (\"children\" in item && item.children.length) {\n          item.children.map(function (element) {\n            element.selected = true;\n            vm.pushSpliceItem(element);\n          });\n        }\n      } else {\n        if (\"children\" in item && item.children.length) {\n          item.children.map(function (element) {\n            element.selected = false;\n            vm.pushSpliceItem(element);\n          });\n        }\n      }\n      vm.toggleParentItem();\n    },\n    toggleParentItem() {\n      this.$emit(\"toggleParentItem\");\n    },\n    parentItemToggle() {\n      let vm = this;\n      if (this.item.naics_code != \"Select All\") {\n        if (\"children\" in this.item && this.item.children.length) {\n          const all_selected = this.item.children.some(child => child.selected);\n          this.item.selected = all_selected;\n        } else {\n          this.item.selected = false;\n        }\n        vm.pushSpliceItem(this.item);\n      }\n    },\n    pushSpliceItem(item) {\n      if (\"naics_id\" in item) {\n        let naics_id = item.naics_id;\n        let selected_naics = [];\n        if (this.$store.getters.selected_naics) {\n          selected_naics = this.$store.getters.selected_naics;\n          if (item.selected) {\n            if (!selected_naics.includes(naics_id)) {\n              selected_naics.push(naics_id);\n            }\n          } else {\n            let naics = selected_naics.filter(function (element) {\n              return element != naics_id;\n            });\n            selected_naics = naics;\n          }\n        } else {\n          if (naics_id) {\n            selected_naics.push(naics_id);\n          }\n        }\n        this.$store.dispatch(\"setSelectedNaics\", selected_naics);\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","props